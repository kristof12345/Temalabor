<Page
    x:Class="Desktop.Views.PlanePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Desktop.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>

        <Button Content="Button" HorizontalAlignment="Left" Margin="339,200,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Image x:Name="planeImg" Source="/Assets/planelogo.jpg" HorizontalAlignment="Left" Height="354" Margin="500,180,0,0" VerticalAlignment="Top" Width="649"/>
        <TextBox x:Name="tb" HorizontalAlignment="Left" Margin="208,313,0,0" Text="TextBox" VerticalAlignment="Top" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Page>
